a, b = input('Введите два целых числа: ').split()
def new_power():
    return int(a) ** (int(b))
print(f'Число {a} в степени {b} = {new_power()}')

args = input().split()
def new_sort():
    for i in args:
        if i == 'r':
            args.sort()
            args.reverse()
            args.remove('r')
            return args
print(new_sort())

N = int(input())
def fac():
    factorial = 1
    for i in range(1,N+1):
        factorial *= i
    print(factorial)
fac()
